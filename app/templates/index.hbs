<p>Lista de Curriculums creados:</p>

<p>
<button {{action 'crearNuevo'}} class='btn btn-success'>Crear</button>
</p>

<table class='table table-bordered'>
  <tr>
    <th> Nombre </th>
    <th> Apellido </th>
  </tr>

{{#if model}}

  {{#each p in model}}
    <tr>
      <td> {{p.nombre}} </td>
      <td> {{p.apellido}} </td>
    </tr>
  {{/each}}

{{else}}

  <tr>
    <td colspan=2>
      No hay <em>curriculums</em> creados.
    </td>
  </tr>

{{/if}}





</table>

{{outlet}}
